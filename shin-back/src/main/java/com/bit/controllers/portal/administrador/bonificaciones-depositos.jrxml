	<queryString>
		<![CDATA[SELECT shinshin.catalogo_medios_bonificacion.nombre_medio_bonificacion as tipo,
	shinshin.historico_medios_bonificacion.fecha_bonificacion as fecha,
	sum(shinshin.historico_medios_bonificacion.cantidad_bonificacion) as importe,
	count(shinshin.historico_medios_bonificacion.id_historico_medios_bonificacion) as solicitudes
FROM shinshin.historico_medios_bonificacion
	INNER JOIN shinshin.medios_bonificacion ON 
	 shinshin.historico_medios_bonificacion.id_medios_bonificacion = shinshin.medios_bonificacion.id_medios_bonificacion 
	INNER JOIN shinshin.catalogo_medios_bonificacion ON 
